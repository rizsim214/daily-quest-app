<?xml version="1.0" encoding="UTF-8" ?>
<!DOCTYPE struts PUBLIC
        "-//Apache Software Foundation//DTD Struts Configuration 2.5//EN"
        "http://struts.apache.org/dtds/struts-2.5.dtd">
<!--
  - This file is included by the struts.xml file as an example
  - of how to break up the configuration file into multiple files.
-->
<struts>
    <package name="transaction" namespace="/" extends="struts-default">
        <action name="acceptQuest" class="com.freedev.dailyquest.transactions.AcceptQuestTransaction">
            <result name="success" type="chain">get_all_quests</result>
            <result name="input" type="redirect">get_all_quests</result>
        </action>
        <action name="QuestSeekerList" class="com.freedev.dailyquest.transactions.GetAllSeekers">
            <result name="success" type="chain">seeker_list</result>
        </action>
    </package>
</struts>
